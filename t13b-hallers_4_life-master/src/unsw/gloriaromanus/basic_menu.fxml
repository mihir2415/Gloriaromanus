         <Button fx:id="trainLegionary" mnemonicParsing="false" onAction="#createLegionary" text="Train" />
      </children>
   </HBox>
   <HBox prefHeight="18.0" prefWidth="270.0">
      <children>
         <Label prefHeight="17.0" prefWidth="132.0" text="Germanic berserker" />
