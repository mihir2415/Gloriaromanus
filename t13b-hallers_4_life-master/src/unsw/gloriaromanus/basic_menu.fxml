         <Button fx:id="trainCeltic" mnemonicParsing="false" onAction="#createCeltic" text="Train" />
      </children>
   </HBox>
   <HBox prefHeight="18.0" prefWidth="270.0">
      <children>
         <Label prefHeight="17.0" prefWidth="131.0" text="Gallic berserker" />
         <Label prefHeight="17.0" prefWidth="150.0" text="Cost :    $6" />
         <Button fx:id="trainGallic" mnemonicParsing="false" onAction="#createGallic" text="Train" />
      </children>
   </HBox>
   <HBox prefHeight="18.0" prefWidth="270.0">
      <children>
         <Label prefHeight="17.0" prefWidth="131.0" text="Javelin Skirmishers" />
         <Label prefHeight="17.0" prefWidth="150.0" text="Cost :    $5" />
         <Button fx:id="trainJavelin" mnemonicParsing="false" onAction="#createJavelin" text="Train" />
      </children>
   </HBox>
   <TextArea fx:id="resultBox" prefHeight="69.0" prefWidth="432.0" />
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button alignment="CENTER" onAction="#clickedInavsionMenuButton" prefHeight="48.0" prefWidth="119.0" text="Inavsion Menu" />
         <Label prefHeight="51.0" prefWidth="165.0" />
         <Button mnemonicParsing="false" onAction="#clickedUnitMenuButton" prefHeight="51.0" prefWidth="123.0" text="Unit Menu" />
      </children>
   </HBox>
   <Button fx:id="displayButton" mnemonicParsing="false" onMousePressed="#clickedDisplay" onMouseReleased="#releasedDisplay" text="Display" />
   <Button fx:id="endGameButton" mnemonicParsing="false" onAction="#clickedEndGame" text="END GAME" />
</VBox>
