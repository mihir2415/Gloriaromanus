<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxWidth="638.0" prefHeight="645.0" prefWidth="638.0" spacing="10" stylesheets="/unsw/gloriaromanus/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unsw.gloriaromanus.select3FactionsController">
   <children>
      <Label prefHeight="288.0" prefWidth="598.0" text="                         Choose Factions " textAlignment="CENTER">
         <font>
            <Font name="Phosphate Inline" size="33.0" />
         </font>
      </Label>
      <HBox prefHeight="123.0" prefWidth="598.0">
         <children>
            <ComboBox fx:id="player1ComboBox" onAction="#clickPlayer1" onMouseClicked="#clickPlayer1" prefHeight="71.0" prefWidth="155.0" promptText="      Player 1 Faction" />
            <Label prefHeight="17.0" prefWidth="39.0" />
            <ComboBox fx:id="player2ComboBox" layoutX="10.0" layoutY="10.0" onAction="#clickPlayer2" onMouseClicked="#clickPlayer2" prefHeight="71.0" prefWidth="172.0" promptText="           Player 2 Faction">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></ComboBox>
            <Label prefHeight="17.0" prefWidth="39.0" />
            <ComboBox fx:id="player3ComboBox" layoutX="233.0" layoutY="10.0" onAction="#clickPlayer3" onMouseClicked="#clickPlayer3" prefHeight="71.0" prefWidth="163.0" promptText="         Player 3 Faction">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ComboBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="addPlayerButton" mnemonicParsing="false" onAction="#clickedAddPlayer" prefHeight="78.0" prefWidth="201.0" text="Add Player" />
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</VBox>
